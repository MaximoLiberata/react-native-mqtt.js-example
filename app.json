{
  "expo": {
    "name": "mqtt-example",
    "slug": "mqtt-example",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "3.0.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "scheme": "mqtt-example",
    "splash": {
      "image": "./assets/logo_1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.example.mqttexample"
    },
    "android": {
      "package": "com.example.mqttexample",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_432.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
